<% include partials/header %>
<div class="col-md-4"></div><div class="col-md-4 box text">
<body>
	<h1>Search Results</h1>
	<ul>
		<% itemList.forEach(function(item){ %>
		<li><a href="/item?itemId=<%=item.itemId%>"><img src="<%= item.galleryURL %>"></a>
		</li>
		<% if(isAuthenticated) {%>
		<form action="/save" method = "POST">
			<input class="input" type="hidden" name="<%=item.itemId%>">
			<input class="input btn btn-primary btn-sm" type="submit" value="Save this search result">
		</form>
		<% } %>
		<% }) %>
	</ul>
</body>
</div>
</html>